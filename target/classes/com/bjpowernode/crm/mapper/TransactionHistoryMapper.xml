<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
 PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
 "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.bjpowernode.crm.mapper.TransactionHistoryMapper">

    <insert id="add">
        insert  into tbl_transaction_history vales(
           #{id},
           #{stage},
           #{amountOfMoney},
           #{expectedClosingDate},
           #{editTime},
           #{editBy},
           #{transactionId}
        )
    </insert>
</mapper>